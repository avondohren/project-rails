<h1>Add New Student</h1>

<% if @student %>
  <% @student.errors.full_messages.each do |error_msg| %>
    <p><%= error_msg %></p>
  <% end %>
<% end %>
  
<form method="post" action="/students">
  <label>First Name</label>
  <input type="text" name="fname">
  
  <label>Last Name</label>
  <input type="text" name="lname">
  
  <input type="submit">
</form>